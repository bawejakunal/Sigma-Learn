<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<context-param>
		<param-name>allowCasualMultipartParsing</param-name>
		<param-value>true</param-value>
	</context-param>

	<servlet>
		<servlet-name>CreateTranscript</servlet-name>
		<servlet-class>CreateTranscriptServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>FileAudio</servlet-name>
		<servlet-class>FileAudioServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>CreateTranscript</servlet-name>
		<url-pattern>/createtranscript</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>FileAudio</servlet-name>
		<url-pattern>/fileaudio</url-pattern>
	</servlet-mapping>

</web-app>
